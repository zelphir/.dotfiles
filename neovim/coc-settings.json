{
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 500,
  "suggest.noselect": false,
  "suggest.detailField": "menu",
  "suggest.snippetIndicator": " ",
  "suggest.triggerCompletionWait": 100,
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "function": "",
    "method": " method",
    "variable": " variable",
    "constant": " constant",
    "struct": "פּ struct",
    "class": " class",
    "interface": "禍interface",
    "text": "",
    "enum": " enum",
    "enumMember": " enumMber",
    "module": " method",
    "color": "",
    "property": "襁property",
    "field": "綠field",
    "unit": " unit",
    "file": "",
    "value": " value",
    "event": "鬒",
    "folder": "",
    "keyword": "",
    "snippet": "",
    "operator": "洛",
    "reference": "  ref",
    "typeParameter": "",
    "default": ""
  },

  //diagnostic
  // "diagnostic.errorSign": "●",
  // "diagnostic.warningSign": "●",
  // "diagnostic.hintSign": "",
  // "diagnostic.infoSign": "",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.enableSign": false,
  "codeLens.enable": true,

  //git
  "git.untrackedDecorator": "",
  "git.changedDecorator": "",
  "git.stagedDecorator": "",
  "git.conflictedDecorator": "",
  "git.enableGutters": true,
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": " ❯❯❯ ",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addedSign.text": "▎",
  "git.changedSign.text": "▎",
  "git.removedSign.text": "▎",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "▎",

  //Snippet
  "coc.preferences.snippetStatusText": "Ⓢ ",

  //CocList
  "list.source.files.defaultOptions": ["--auto-preview"],
  "list.source.outline.defaultOptions": ["--auto-preview"],

  //prettier
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "go",
    "javascript",
    "javascript.jsx",
    "typescript.jsx",
    "md",
    "scss",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "prettier.statusItemText": "P ",
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,

  //emmet
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},

  //multiple curssors
  "cursors.nextKey": "<C-n>",
  "cursors.previousKey": "<C-p>",
  "cursors.cancelKey": "<esc>",

  //sessions
  "session.directory": "~/.cache/nvim/session",

  // coc-explorer
  "explorer.keyMappings": {
    "openInVsplit": "v",
    "openInSplit": "s"
  },
  "explorer.file.autoExpandSingleDirectory": false,
  "explorer.icon.expanded": "▾",
  "explorer.icon.shrinked": "▸",
  "explorer.icon.enableNerdfont": true,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.buffer.columns": [
    "selection",
    "name",
    "modified",
    "readonly"
  ],
  "explorer.file.columns": [
    "git",
    "selection",
    "clip",
    "diagnosticError",
    "indent",
    "icon",
    "filename",
    "readonly"
  ],
  "explorer.file.showHiddenFiles": true,
  // "explorer.file.column.git.icon.mixed": "*",
  // "explorer.file.column.git.icon.unmodified": "",
  // "explorer.file.column.git.icon.modified": "•",
  "explorer.file.column.git.icon.added": "+",
  // "explorer.file.column.git.icon.deleted": "˗",
  // "explorer.file.column.git.icon.renamed": "R",
  // "explorer.file.column.git.icon.copied": "C",
  // "explorer.file.column.git.icon.unmerged": "U",
  // "explorer.file.column.git.icon.untracked": "?",
  // "explorer.file.column.git.icon.ignored": "!",

  // eslint
  "eslint.filetypes": ["javascript", "javascript.jsx", "typescript", "typescriptreact", "javascriptreact"],
  "eslint.packageManager": "yarn",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,

  // custom languageservers
  "languageserver": {}
}
