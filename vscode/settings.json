{
    "docker.enableLinting": true,
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "Operator Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.tabSize": 2,
    "extensions.autoUpdate": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "indenticator.showCurrentDepthInStatusBar": false,
    "javascript.validate.enable": false,
    "jsAutolint.defaultLinters": [
        "standard"
    ],
    "jsAutolint.showStatus": false,
    "prettier.singleQuote": true,
    "standard.enable": false,
    "standard.nodePath": "/Users/roberto/.config/yarn/global/node_modules/.bin",
    "standard.options": {
        "ignore": [
            "node_modules/**"
        ],
        "parser": "babel-eslint"
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.fontFamily": "FuraCodeNerdFontO-Light",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.rightClickCopyPaste": true,
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Fira Code",
    "vim.easymotionMarkerFontSize": "14",
    "vim.easymotionMarkerForegroundColorOneChar": "#ec5f67",
    "vim.easymotionMarkerForegroundColorTwoChar": "#99c794",
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerWidthPerChar": 9,
    "vim.easymotionMarkerYOffset": 14,
    "vim.enableNeovim": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "insert": "#85678f",
        "normal": "#5f819d",
        "replace": "#a54242",
        "visual": "#8c9440",
        "visualblock": "#5e8d87",
        "visualline": "#8c9440"
    },
    "vim.useSystemClipboard": true,
    "vscode_custom_css.imports": [
        "file:////Users/roberto/.dotfiles/vscode/custom.css"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.jsonOfficial": true,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#85678f",
        "statusBar.noFolderBackground": "#85678f",
        "statusBar.debuggingBackground": "#85678f"
    },
    "workbench.colorTheme": "Snazzy Operator",
    "workbench.iconTheme": "vscode-icons",
    "vim.disableAnnoyingGcMessage": true,
    "typescript.check.npmIsInstalled": false
}
