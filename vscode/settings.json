{
    "docker.enableLinting": true,
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "Operator Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.tabSize": 2,
    "extensions.autoUpdate": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "javascript.validate.enable": false,
    "jsAutolint.defaultLinters": [
        "standard"
    ],
    "jsAutolint.showStatus": false,
    "prettier.singleQuote": true,
    "standard.enable": false,
    "standard.nodePath": "/Users/roberto/.config/yarn/global/node_modules/.bin",
    "standard.options": {
        "ignore": [
            "node_modules/**"
        ],
        "parser": "babel-eslint"
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.fontFamily": "FuraCodeNerdFontO-Light",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.rightClickCopyPaste": true,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Fira Code",
    "vim.easymotionMarkerFontSize": "14",
    "vim.easymotionMarkerForegroundColorOneChar": "#ec5f67",
    "vim.easymotionMarkerForegroundColorTwoChar": "#99c794",
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerWidthPerChar": 9,
    "vim.easymotionMarkerYOffset": 14,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "insert": "#85678f",
        "normal": "#5f819d",
        "replace": "#a54242",
        "visual": "#8c9440",
        "visualblock": "#5e8d87",
        "visualline": "#8c9440"
    },
    "vim.useSystemClipboard": true,
    "vscode_custom_css.imports": [
        "file:////Users/roberto/.dotfiles/vscode/custom.css"
    ],
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.jsonOfficial": true,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#282a2d",
        "terminal.ansiBlue": "#5f819d",
        "terminal.ansiBrightBlack": "#373b41",
        "terminal.ansiBrightBlue": "#81a2be",
        "terminal.ansiBrightCyan": "#8abeb7",
        "terminal.ansiBrightGreen": "#b5bd68",
        "terminal.ansiBrightMagenta": "#b294bb",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiBrightWhite": "#c5c8c6",
        "terminal.ansiBrightYellow": "#f0c674",
        "terminal.ansiCyan": "#5e8d87",
        "terminal.ansiGreen": "#8c9440",
        "terminal.ansiMagenta": "#85678f",
        "terminal.ansiRed": "#a54242",
        "terminal.ansiWhite": "#707880",
        "terminal.ansiYellow": "#de935f",
        "terminal.background": "#1d1f21",
        "terminal.foreground": "#c5c8c6"
    },
    "workbench.colorTheme": "Hybrid Operator",
    "workbench.iconTheme": "vscode-icons"
}
