"*":
  "atom-clock":
    dateFormat: "ddd DD MMM - HH:mm"
  "autocomplete-plus":
    suppressActivationForEditorClasses: [
      "vim-mode-plus.normal-mode"
      "vim-mode-plus.visual-mode"
      "vim-mode-plus.operator-pending-mode"
      "vim-mode-plus.insert-mode.replace"
    ]
  autosave:
    enabled: true
  "battery-status": {}
  blame: {}
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "nuclide"
      "whitespace"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "gruvbox"
    ]
  docblockr: {}
  editor:
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "6c6a98d9-2111-055b-3833-19630a0052fc"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "find-and-replace": {}
  "fuzzy-finder":
    preserveLastSearch: true
  "git-blame":
    ignoreWhiteSpaceDiffs: true
  "highlight-selected": {}
  "isotope-ui":
    fontWeight: "Extra light / Thin"
    minimalMode: true
  "js-hyperclick": {}
  linter: {}
  "linter-eslint":
    globalNodePath: "/Users/roberto/.n"
  "linter-js-standard":
    honorStyleSettings: true
    lintMarkdownFiles: true
  "linter-tidy":
    executablePath: "/usr/local/bin/tidy"
  "linter-ui-default": {}
  "markdown-preview":
    useGitHubStyle: true
  minimap:
    plugins:
      "split-diff": true
      "split-diffDecorationsZIndex": 0
  "one-dark-ui":
    fontSize: 11
    tabSizing: "Even"
  pigments:
    extendAutocompleteToVariables: true
  "prettier-atom":
    singleQuote: true
  react:
    enabledForAllJavascriptFiles: true
  "split-diff":
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "standard-formatter": {}
  "status-bar": {}
  "tab-control": {}
  tabs:
    enableVcsColoring: true
    usePreviewTabs: true
  "tabs-to-spaces":
    onSave: "untabify"
  term3:
    colors:
      background: "#121e25"
    fontFamily: "FuraCode Nerd Font"
    fontSize: "14"
    openPanesInSameSplit: true
  "trailing-spaces":
    enableForCursorLines: true
    enableForIndentation: true
  "tree-view":
    hideIgnoredNames: true
  "vim-mode-plus":
    clearMultipleCursorsOnEscapeInsertMode: true
    debug: true
    flashOnOperate: false
    flashOnUndoRedoDuration: 150
    highlightSearch: true
    ignoreCaseForSearch: true
    ignoreCaseForSearchCurrentWord: true
    incrementalSearch: true
    setCursorToStartOfChangeOnUndoRedo: true
    showHoverOnOperate: true
    showHoverSearchCounter: true
    statusBarModeStringStyle: "long"
    useSmartcaseForSearch: true
    useSmartcaseForSearchCurrentWord: true
  wakatime:
    apikey: "Saved in your ~/.wakatime.cfg file"
    showStatusBarIcon: false
  "wakatime-hidden":
    lastInit: 1505398611
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".js.jsx.source":
  editor:
    autoIndentOnPaste: true
